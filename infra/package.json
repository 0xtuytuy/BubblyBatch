{
  "name": "kefir-app-infra",
  "version": "0.1.0",
  "description": "Infrastructure as Code for Kefir App using SST",
  "type": "module",
  "scripts": {
    "dev": "sst dev",
    "build": "sst build",
    "deploy": "sst deploy",
    "deploy:dev": "sst deploy --stage dev",
    "deploy:prod": "sst deploy --stage prod",
    "remove": "sst remove",
    "remove:dev": "sst remove --stage dev",
    "remove:prod": "sst remove --stage prod",
    "console": "sst console",
    "logs": "sst logs",
    "types": "sst types",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "sst": "^3.3.0",
    "aws-cdk-lib": "^2.161.0",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

